{
  "name": "gen-api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "bin": {
    "gen-api": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts --tsconfig tsconfig.json",
    "start": "node dist/index.js"
  },
  "description": "CLI to scaffold API with NodeJS / TypeScript setup.",
  "homepage": "https://github.com/titenq/gen-api#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/titenq/gen-api.git"
  },
  "bugs": {
    "url": "https://github.com/titenq/gen-api/issues"
  },
  "author": "TitenQ <titenq@gmail.com> (https://github.com/titenq)",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^24.1.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^17.2.0"
  },
  "keywords": [
    "cli",
    "typescript",
    "node",
    "nodejs",
    "starter",
    "scaffold",
    "tsup",
    "tsx",
    "dotenv",
    "tsconfig",
    "boilerplate",
    "project-init",
    "npm-init",
    "project-generator"
  ]
}
